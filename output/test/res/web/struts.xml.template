<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
    <constant name="struts.enable.DynamicMethodInvocation" value="true" />
    <constant name="struts.devMode" value="@STRUTS_DEVMODE@" />
    <constant name="struts.action.extension" value="go" />
    <constant name="struts.serve.static" value="true" />
    <constant name="struts.ui.theme" value="css_xhtml"/>
    <constant name="struts.i18n.encoding" value="UTF-8"/>
    <constant name="struts.serve.static.browserCache" value="true"/>
    <constant name="struts.i18n.reload" value="false"/>
    <constant name="struts.xslt.nocache" value="false"/>

    <include file="com/i10n/fleet/web/fleet-default.xml"/>
    <include file="com/i10n/fleet/web/view.xml"/>
    <include file="com/i10n/fleet/web/track.xml"/>
    <include file="com/i10n/fleet/web/report.xml"/>
    <include file="com/i10n/fleet/web/fencing.xml"/>
    <include file="com/i10n/fleet/web/admin.xml"/>
    <include file="com/i10n/fleet/web/landmarks.xml"/>
    <include file="com/i10n/fleet/web/upload.xml"/>
    <include file="com/i10n/fleet/web/fleetmobile.xml"/>
    <include file="com/i10n/fleet/web/scheduling.xml"/>
    <include file="com/i10n/fleet/web/u.xml"/>
</struts>